{
  "name": "deckgl-typings",
  "version": "1.0.0",
  "description": "TypeScript declaration files for deck.gl",
  "private": true,
  "scripts": {
    "add-tsconfig": "shx cp tsconfig.json ./node_modules/deck.gl/.",
    "to-ts": "shx cp ./node_modules/deck.gl/src/index.js ./node_modules/deck.gl/src/index.ts",
    "dts": "dts-generator --name deck.gl --project ./node_modules/deck.gl --out deck.gl/index.d.ts",
    "build": "npm run add-tsconfig && npm run to-ts && npm run dts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danmarshall/deckgl-typings.git"
  },
  "keywords": [
    "deck.gl",
    "TypeScript"
  ],
  "author": "Dan Marshall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danmarshall/deckgl-typings/issues"
  },
  "homepage": "https://github.com/danmarshall/deckgl-typings#readme",
  "devDependencies": {
    "dts-generator": "^2.1.0",
    "shx": "^0.3.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "deck.gl": "^5.3.2",
    "luma.gl": "^5.3.0"
  }
}
